{
  "name": "opentok-accelerator-core",
  "version": "2.0.17",
  "description": "Opentok Accelerator Core",
  "repository": "https://github.com/opentok/accelerator-core-js",
  "main": "dist/core.js",
  "scripts": {
    "old:build:node": "babel -d ./dist/ ./src/",
    "old:build:browser-polyfill": "(echo \"require('babel-polyfill');\\n\"; cat src/core.js) > src/browser-temp.js",
    "old:build:browser-browserify": "mkdir -p browser && browserify src/browser-temp.js -o browser/opentok-acc-core.js -t [ babelify --presets [ es2015 ] --plugins [ transform-class-properties ] ] --im",
    "old:build:browser": "npm run build:browser-polyfill && npm run build:browser-browserify && rm src/browser-temp.js",
    "old:build:logversion": "replace '(js-vsol-)(x.y.z)' '$1'$npm_package_version dist/* browser/*",
    "old:build": "npm run build:node && npm run build:browser && npm run build:logversion",
    "old:test": "karma start",
    "build": "tsc",
    "build:logversion": "replace '(js-vsol-)(x.y.z)' '$1'$npm_package_version build/*",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc '**/*.ts' --write",
    "test": "mocha -r ts-node/register test/**/*spec.ts --insect",
    "coverage": "nyc mocha -r ts-node/register test/**/*spec.ts --insect",
    "browserify:sdk": "browserify -p tinyify build/src/sdk-wrapper/sdkWrapper.js -o dist/sdk.js"
  },
  "keywords": [
    "opentok",
    "accelerator",
    "core",
    "opentok-acc-pack",
    "webrtc",
    "communication",
    "video",
    "audio"
  ],
  "author": "michael.jolley@vonage.com",
  "license": "MIT",
  "devDependencies": {
    "@opentok/client": "^2.18.2",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.14.6",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "tinyify": "^3.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1"
  },
  "dependencies": {
    "opentok-solutions-logging": "^1.0.7"
  }
}