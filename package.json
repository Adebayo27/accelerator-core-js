{
  "name": "opentok-accelerator-core",
  "version": "2.0.15",
  "description": "Opentok Accelerator Core",
  "repository": "https://github.com/opentok/accelerator-core-js",
  "main": "dist/core.js",
  "scripts": {
    "old:build:node": "babel -d ./dist/ ./src/",
    "old:build:browser-polyfill": "(echo \"require('babel-polyfill');\\n\"; cat src/core.js) > src/browser-temp.js",
    "old:build:browser-browserify": "mkdir browser && browserify src/browser-temp.js -o browser/opentok-acc-core.js -t [ babelify --presets [ es2015 ] --plugins [ transform-class-properties ] ] --im",
    "old:build:browser": "npm run build:browser-polyfill && npm run build:browser-browserify && rm src/browser-temp.js",
    "old:build:logversion": "replace '(js-vsol-)(x.y.z)' '$1'$npm_package_version ./dist ./browser",
    "old:build": "npm run build:node && npm run build:browser && npm run build:logversion",
    "old:test": "karma start",
    "build": "tsc",
    "test": ""
  },
  "keywords": [
    "opentok",
    "accelerator",
    "core",
    "opentok-acc-pack",
    "webrtc",
    "communication",
    "video",
    "audio"
  ],
  "author": "Michael Jolley <michael.jolley@vonage.com>",
  "license": "MIT",
  "devDependencies": {
    "@opentok/client": "^2.18.0",
    "@types/node": "^14.11.2",
    "chai": "^4.1.2",
    "mocha": "^8.1.3",
    "opentok-annotation": "^2.0.55",
    "opentok-archiving": "^1.0.19",
    "opentok-screen-sharing": "^1.0.27",
    "opentok-text-chat": "^1.0.30",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "opentok-solutions-logging": "^1.1.0"
  }
}
